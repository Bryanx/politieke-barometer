            <div></div>
        </div>
        <div data-gs-x="4" data-gs-y="4" data-gs-width="4" data-gs-height="3" class="grid-stack-item ui-draggable ui-resizable ui-resizable-autohide">
            <div class="grid-stack-item-content ui-draggable-handle"></div>
            <div></div>
        </div>
        <div data-gs-x="3" data-gs-y="1" data-gs-width="6" data-gs-height="2" class="grid-stack-item ui-draggable ui-resizable ui-resizable-autohide">
            <div class="grid-stack-item-content ui-draggable-handle"></div>
            <div></div>
        </div>
        <div data-gs-x="0" data-gs-y="3" data-gs-width="2" data-gs-height="6" class="grid-stack-item ui-draggable ui-resizable ui-resizable-autohide">
            <div class="grid-stack-item-content ui-draggable-handle"></div>
            <div></div>
        </div>
    </div>
</div>