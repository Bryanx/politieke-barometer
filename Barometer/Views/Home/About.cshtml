@{
    ViewBag.Title = "About";
}

<style type="text/css">
    .grid-stack {
        position: relative;
        width: 960px;
    }
    .grid-stack-item-content {
        background-color: darkgray;
        border: 1px white;
    }
</style>
<style type="text/css" data-gs-style-id="gridstack-style-5124"></style>
<div class="container-fluid">
    <h1>Float grid demo</h1>

    <div>
        <a class="btn btn-default" id="add-new-widget" href="#">Add Widget</a>
